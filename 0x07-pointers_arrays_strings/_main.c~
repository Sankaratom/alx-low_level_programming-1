#include <unistd.h>
#include <stdio.h>
#include <stdlib.h>
#include "holberton.h"  

int _putchar(char c)
{
  return (write(1, &c, 1));
}

int main(void)
{
	char buf[100] = {0};
	
	printf("Testing 0\n");
	_memset(buf, 'b', 20);
	buf[25] = '\0';
	printf("buf: %s\n",buf);

	printf("\nTesting 1\n");
	
	
	return (0);
}
